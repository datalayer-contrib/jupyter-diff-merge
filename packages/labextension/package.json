{
  "name": "nbdime-jupyterlab",
  "version": "3.0.0",
  "description": "A JupyterLab extension for showing Notebook diffs.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "notebook"
  ],
  "homepage": "https://github.com/jupyter/nbdime",
  "bugs": {
    "url": "https://github.com/jupyter/nbdime/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyter/nbdime"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "files": [
    "lib/*.js",
    "lib/*.js.map",
    "lib/*.d.ts",
    "style/*.css",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "npm run build:lib && npm run build:labextension",
    "build:dev": "npm run build:lib",
    "build:labextension": "rimraf dist && mkdirp dist && cd dist && npm pack ..",
    "build:lib": "tsc --build",
    "clean": "rimraf lib && rimraf dist",
    "prepublishOnly": "npm run build",
    "update": "rimraf node_modules/nbdime && npm install && npm run build",
    "watch": "tsc --build --watch"
  },
  "dependencies": {
    "@jupyterlab/apputils": "^3.0.0-rc.13",
    "@jupyterlab/coreutils": "^5.0.0-rc.13",
    "@jupyterlab/nbformat": "^3.0.0-rc.13",
    "@jupyterlab/notebook": "^3.0.0-rc.13",
    "@jupyterlab/rendermime": "^3.0.0-rc.13",
    "@jupyterlab/services": "^6.0.0-rc.13",
    "@jupyterlab/settingregistry": "^3.0.0-rc.13",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/disposable": "^1.4.3",
    "@lumino/widgets": "^1.16.0",
    "@datalayer-jupyter/nbdime": "^7.0.0"
  },
  "devDependencies": {
    "@jupyterlab/application": "^3.0.0-rc.13",
    "@jupyterlab/docregistry": "^3.0.0-rc.13",
    "@lumino/commands": "^1.6.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.3",
    "typescript": "^3.7.2"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "discovery": {
      "server": {
        "base": {
          "name": "nbdime"
        },
        "managers": [
          "pip",
          "conda"
        ]
      }
    }
  },
  "resolutions": {
    "**/react": "^17.0.1",
    "**/react-dom": "^17.0.1"
  }
}
